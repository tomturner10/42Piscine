rush-01:
	gcc -Wall -Wextra -Werror -o ex00/rush01 ex00/*.c
	./ex00/rush01  "4 3 2 1 1 2 2 2 4 3 2 1 1 2 2 2"
	./ex00/rush01 "2 1 2 2 3 4 1 2 2 1 2 2 3 4 1 2"
rush-01bad:
	gcc -Wall -Wextra -Werror -o ex00/rush01 *.c
	./ex00/rush01 "4 3 a 1 1 2 2 2 4 3 2 1 1 2 2 2"
	./ex00/rush01 ""
	./ex00/rush01
norm:
	norminette *.c